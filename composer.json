{
    "name": "roots/bedrock",
    "type": "project",
    "license": "MIT",
    "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
    "homepage": "https://roots.io/bedrock/",
    "authors": [{
        "name": "Scott Walkinshaw",
        "email": "scott.walkinshaw@gmail.com",
        "homepage": "https://github.com/swalkinshaw"
    }, {
        "name": "Ben Word",
        "email": "ben@benword.com",
        "homepage": "https://github.com/retlehs"
    }],
    "keywords": [
        "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
    ],
    "support": {
        "issues": "https://github.com/roots/bedrock/issues",
        "forum": "https://discourse.roots.io/category/bedrock"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "repositories": [{
        "type": "composer",
        "url": "https://wpackagist.org"
    }, {
        "type": "composer",
        "url": "http://www.menthalia.com/premiumpress/satispress/packages.json"
    }, {
        "type": "package",
        "package": {
            "name": "khromov/wp-performance-profiler",
            "version": "0.3",
            "type": "wordpress-muplugin",
            "dist": {
                "url": "https://github.com/khromov/wp-performance-profiler/archive/0.3.zip",
                "type": "zip"
            },
            "source": {
                "url": "https://github.com/khromov/wp-performance-profiler.git",
                "type": "git",
                "reference": "dev-master"
            },
            "require": {
                "composer/installers": "~1.0.12"
            }
        }
    }, {
        "type": "package",
        "package": {
            "name": "khromov/wp-request-buster",
            "version": "1.0",
            "type": "wordpress-plugin",
            "dist": {
                "url": "https://github.com/khromov/wp-request-buster/archive/1.0.zip",
                "type": "zip"
            },
            "source": {
                "url": "https://github.com/khromov/wp-request-buster.git",
                "type": "git",
                "reference": "dev-master"
            },
            "require": {
                "composer/installers": "~1.0.12"
            }
        }
    }, {
        "type": "vcs",
        "url": "https://github.com/pressjitsu/wp-transients-cleaner"
    }],
    "require": {
        "php": ">=5.6",
        "composer/installers": "~1.0.12",
        "vlucas/phpdotenv": "^2.0.1",
        "johnpbloch/wordpress": "4.7.2",
        "oscarotero/env": "^1.0",
        "roots/wp-password-bcrypt": "1.0.0",


        "satispress/wooCommerce-dynamic-pricing": "*",
        "wpackagist-plugin/password-protected": "*",
        "wpackagist-plugin/post-2-post-for-acf": "*",
        "satispress/divi-builder": "*",
        "wpackagist-plugin/tinywym-editor": "*",


        "satispress/gravityforms": "*",

        "wpackagist-plugin/menu-social-icons": "*",
        "wpackagist-plugin/plugin-organizer": "*",
        "satispress/xslt": "*",
        "satispress/global-meta-box-order": "*",

        "satispress/advanced-custom-fields-pro": "*",
        "wpackagist-plugin/wp-spamshield": "*",
        "satispress/searchwp": "*",
        "wpackagist-plugin/woocommerce": "*",
        "satispress/sitepress-multilingual-cms": "*",
        "wpackagist-plugin/wp-email-template": "*",
        "wpackagist-plugin/disable-comments": "*",
        "wpackagist-plugin/favicon-by-realfavicongenerator": "*",
        "wpackagist-plugin/duplicate-post": "*",
        "wpackagist-plugin/post-types-order": "*",
        "satispress/enhanced-media-library-pro": "*",
        "wpackagist-plugin/enable-media-replace": "*",
        "wpackagist-plugin/term-management-tools": "*",
        "satispress/admin-columns-pro": "*",
        "satispress/cac-addon-acf": "*",
        "satispress/responsive-menu-pro": "1.0.9",

        "satispress/updraftplus": "*",
        "satispress/wp-migrate-db-pro": "*",
        "satispress/wp-migrate-db-pro-media-files": "*",
        "roots/soil": "*",
        "wpackagist-plugin/transients-manager": "*",
        "wpackagist-plugin/ewww-image-optimizer": "*",
        "wpackagist-plugin/safe-svg": "1.3.0",
        "pressjitsu/wp-transients-cleaner": "dev-master",
        "wpackagist-plugin/remove-orphan-shortcodes": "*",
        "wpackagist-plugin/wp-widget-disable": "*",
        "wpackagist-plugin/error-log-monitor": "*",
        "wpackagist-plugin/imsanity": "*",
        "wpackagist-plugin/stops-core-theme-and-plugin-updates": "*",


        "wpackagist-plugin/autodescription": "*",

        "wpackagist-plugin/facebook-open-graph-update": "*",

        "wpackagist-plugin/google-sitemap-generator": "*",


        "wpackagist-plugin/wp-edit": "*",
        "satispress/admin-menu-editor-pro": "*",


        "satispress/wp-toolbar-editor": "*",



        "satispress/searchwp-exclude-ui-master": "*",

        "satispress/searchwp-term-priority": "*",
        "satispress/wpml-media": "*",
        "satispress/wpml-string-translation": "*",
        "satispress/wpml-translation-management": "*",
        "satispress/gravityformsmailchimp": "*",
        "satispress/gravityforms-multilingual": "*",
        "satispress/searchwp-wpml": "*",
        "satispress/woocommerce-multilingual": "*",
        "satispress/cac-addon-woocommerce": "*",
        "satispress/searchwp-woocommerce": "*",
        "satispress/woocommerce-google-analytics-pro": "*"


    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5.1",

        "wpackagist-plugin/custom-post-type-ui": "*",

        "wpackagist-plugin/regenerate-thumbnails": "*",




        "wpackagist-plugin/log-deprecated-notices": "*",
        "wpackagist-plugin/wp-crontrol": "*",
        "wpackagist-plugin/plugins-garbage-collector": "*",
        "wpackagist-plugin/options-inspector": "*",
        "wpackagist-plugin/debug-bar": "*",
        "wpackagist-plugin/query-monitor": "*",

        "wpackagist-plugin/p3-profiler": "*",
        "khromov/wp-performance-profiler": "*",
        "wpackagist-plugin/debug-bar-extender": "*",
        "wpackagist-plugin/debug-bar-slow-actions": "*"

    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
            "web/app/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcs --ignore=web/wp/,vendor/ -n -s ."
        ]
    }
}
